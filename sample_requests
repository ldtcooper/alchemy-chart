// test get all users
$.ajax({
  method: 'GET',
  url: 'api/users'
});

// test user creation
$.ajax({
  method: 'POST',
  url: 'api/users',
  data: {user: {username: 'alvink', password: 'alvink'}}
});

//test logout
$.ajax({
  method: 'DELETE',
  url: 'api/session'
});

// test login
$.ajax({
  method: 'POST',
  url: 'api/session',
  data: {user: {username: 'ldtcooper', password: 'lionhart_01!'}}
});
